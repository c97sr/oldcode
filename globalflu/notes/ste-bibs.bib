@article{Whiley:2009p4688,
author = {David M Whiley and Seweryn Bialasiewicz and Cheryl Bletchly and Cassandra E Faux and Bruce Harrower and Allan R Gould and Stephen B Lambert and Graeme R Nimmo and Michael D Nissen and Theo P Sloots}, 
