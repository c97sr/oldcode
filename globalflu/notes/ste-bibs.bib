keywords = {Logistic Models, Humans, Hepatomegaly, Adolescent, Adult, Male, Endemic Diseases, Risk Factors, Splenomegaly, Middle Aged, Feces, Female, Child, Schistosomiasis japonica, Animals, Parasite Egg Count, Child: Preschool, Occupational Diseases, China, Cross-Sectional Studies}, 
date-added = {2008-12-07 12:32:50 +0800},
