language = {eng},
keywords = {Female, Disease Transmission: Infectious, Markov Chains, Humans, Longitudinal Studies, Influenza A virus, Adult, Monte Carlo Method, Adolescent, Male, Child, Models: Biological, Influenza: Human, Bayes Theorem, Family Characteristics, France, Child: Preschool, Models: Statistical}, 
