language = {eng},
keywords = {Schistosoma japonicum, Animals, Humans, Cross-Sectional Studies, Snails, Linear Models, Schistosomiasis japonica, Philippines, Disease Vectors, Host-Parasite Interactions, Water, Ecosystem}, 
date-added = {2008-12-03 22:41:24 +0800},
date-modified = {2009-11-30 10:20:18 +0800},
